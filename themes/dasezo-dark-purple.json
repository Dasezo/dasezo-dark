    {
      "name": "comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#929790",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python docstring",
      "scope": ["string.quoted.docstring.multi.python"],
      "settings": {
        "foreground": "#717CB4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python builtins",
      "scope": ["support.type.python", "support.function"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Basic foreground",
